<header>
  <h1><a href="/">SearchStats</a></h1>
  <h2>An evaluating tool for writing better CSS</h2>
</header>

<main>
  <div id="js-searchView">
  <div class="container l-input">
    <div class="c-inputBox">
      <div class="c-card">
        <input id="js-query" type="text" placeholder="test log for URLs containing" class="c-input"/>
        <div id="js-progress" class="progress">
          <div class="line"></div>
        </div>
      </div>
      <button type="button" title="Start Test" class="c-button ripple" id="js-search">
        <span class="inner">
          <svg fill="#fff" width="30" height="30" viewbox="0 0 48 48">
            <path d="M31 28h-1.59l-.55-.55C30.82 25.18 32 22.23 32 19c0-7.18-5.82-13-13-13S6 11.82 6 19s5.82 13 13 13c3.23 0 6.18-1.18 8.45-3.13l.55.55V31l10 9.98L40.98 38 31 28zm-12 0c-4.97 0-9-4.03-9-9s4.03-9 9-9 9 4.03 9 9-4.03 9-9 9z"/>
          </svg>
        </span>
      </button>
    </div>
  </div>
  <div class="container">
    <input type="text">
    <input type="text">
    <a href="" class="button">Compare</a>
    <ul>
      {{#each searchResults}}
        <li>
          <input type="checkbox" class="filled-in" data-id="{{ id }}">
          <a href="{{ uri }}">
            <p class="path">{{ path }}</p>
            <p class="date">{{ time }}</p>
          </a>
        </li>
      {{/each}}
    </ul>
  </div>
  </div>
</main>
