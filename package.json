    "handlebars": "^3.0.2",
    "jade": "^1.9.1",
    "logfmt": "^1.2.0",
    "method-override": "^2.3.1",
    "moment": "^2.10.2",
    "morgan": "^1.5.1",
    "newrelic": "^1.14.7",
    "numeral": "^1.5.3",
